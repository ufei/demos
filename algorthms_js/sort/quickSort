var test = [3,1,5,2,10,4];
function quickSort(str){
    var i,j,key,tmp,cursor;
    i = 0;
    j = str.length;
    key = str[0];
    cursor = 0;
    while(i!==j){
        if(str[j]<key){
            tmp = str[j];
            str[j]= key;
            str[i] = tmp;
            cursor = j;
            j--;
        }
        if(str[i]>key){
            tmp = str[]
        }
    }
}
